{
  "name": "zf-commons/zfc-rbac",
  "description": "Zend Framework 2 Module that provides a layer of features of Zend\\Permissions\\Rbac",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "module",
    "zf2",
    "rbac",
    "permissions"
  ],
  "homepage": "http://www.github.com/ZF-Commons/zfc-rbac",
  "authors": [
    {
      "name": "Kyle Spraggs",
      "email": "theman@spiffyjr.me",
      "homepage": "http://www.spiffyjr.me/"
    },
    {
      "name": "MichaÃ«l Gallego",
      "email": "mic.gallego@gmail.com",
      "homepage": "http://www.michaelgallego.fr"
    },
    {
      "name": "Jean-Marie Leroux",
      "email": "jmleroux.pro@gmail.com"
    },
    {
      "name": "Florent Blaison",
      "email": "florent.blaison@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.6",
    "container-interop/container-interop": "^1.1",
    "zendframework/zend-stdlib": "^2.7",
    "zfr/rbac": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.2",
    "squizlabs/php_codesniffer": "^2.5",
    "satooshi/php-coveralls": "~0.6",
    "zendframework/zend-servicemanager": "dev-develop",
    "zendframework/zend-authentication": "^2.6",
    "zendframework/zend-stratigility": "~1.1",
    "zendframework/zend-diactoros": "~1.1",
    "doctrine/common": "^2.5"
  },
  "autoload": {
    "psr-4": {
      "ZfcRbac\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ZfcRbacTest\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.4-dev",
      "dev-develop": "3.0-dev"
    }
  },
  "suggest": {
    "zendframework/zend-authentication": "^2.6 to use zend-authentication as authentication middleware",
    "zendframework/zend-stratigility": "~1.1 : if you need the middleware capability"
  }
}
